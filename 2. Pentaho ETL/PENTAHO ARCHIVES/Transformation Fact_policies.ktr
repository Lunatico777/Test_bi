    <sql>########################## FACT_POLICIES ##########################
with 
po as (
	with 
	policy as (
	SELECT
    project_id,
    max(active_since) as active_since,
    MAX(is_active) as is_active 
	FROM bi_test_last.project_policies
	GROUP BY 1
	),
	maxi_info as (
	SELECT
    project_policy_id,
    max(id_phase) as id_phase
    from bi_test_last.project_policy_info
    where year in (2018,2019)
    ## and description is not null
    group by 1
    )
	select project_id, active_since, is_active, i.year, i.title, i.description, m.id_phase
    from bi_test_last.project_policy_info i 
    join maxi_info m on (concat(m.project_policy_id,  m.id_phase) = concat(i.project_policy_id, i.id_phase))
    join policy p on (p.project_id = i.project_policy_id)
),
lo as (
	SELECT distinct l.id as id_country, name as name_location, iso_alpha_2, iso_numeric, parent_id, project_policy_id
	FROM bi_test_last.loc_elements l
	join bi_test_last.project_policy_countries c on (c.id_country = l.id)

),
scp as (
	SELECT distinct rep_ind_geographic_scope_id as id_scope,
	name as name_scope,
	definition,
	project_policy_id
	FROM bi_test_last.project_policy_geographic_scopes s
	join bi_test_last.rep_ind_geographic_scopes g on (s.rep_ind_geographic_scope_id = g.id)
),
go as (
	SELECT
	g.id as global_unit_id,
	g.smo_code,
	g.name as name_global_unit,
	g.acronym,
    project_policy_id
    FROM bi_test_last.global_units g
    join bi_test_last.project_policy_info i on (g.id = i.global_unit_id)
)
select 
po.project_id, 
po.active_since, 
po.is_active, 
po.year, 
po.title, 
po.description, 
po.id_phase,

lo.id_country, 
lo.name_location, 
lo.iso_alpha_2, 
lo.iso_numeric, 
lo.parent_id, 

scp.id_scope,
scp.name_scope,
scp.definition,

go.global_unit_id,
go.smo_code,
go.name_global_unit,
go.acronym,

amount
from bi_test_last.project_policy_info i
left join po on (po.project_id = i.project_policy_id)
left join lo on (lo.project_policy_id = i.project_policy_id)
left join scp on (scp.project_policy_id = i.project_policy_id)
left join go on (go.project_policy_id = i.project_policy_id)
where project_id is not null
;</sql>
